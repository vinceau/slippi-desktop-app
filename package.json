    "connected-react-router": "^5.0.1",
    "cross-env": "^5.2.0",
    "cross-spawn": "^6.0.5",
    "css-loader": "^1.0.1",
    "detect-port": "^1.3.0",
    "electron": "^4.1.4",
    "electron-builder": "^20.34.0",
